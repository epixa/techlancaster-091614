<!DOCTYPE html>
<html lang="en" ng-app="app">
<head>
  <meta charset="utf-8" />
  <title>Tech Lancaster</title>

  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <base href="/">

  <link href="/static/assets/css/styles.css" rel="stylesheet" />
  <script src="/static/assets/js/angular/angular.js" type="text/javascript"></script>
</head>
<body>

  <header>
    <h1>Tech Lancaster</h1>

    <ul class="breadcrumbs" ng-controller="BreadcrumbsCtrl">
      <li ng-if="!current.name">Home</li>
      <li ng-if="current.name"><a href="/">Home</a></li>
      <li ng-if="current.name">
        &raquo;&nbsp; {{ current.name }}
      </li>
    </ul>

  </header>

  <div ng-view></div>

  <script src="/static/assets/js/angular-route/angular-route.js" type="text/javascript"></script>

  <script src="/static/app.js" type="text/javascript"></script>
  <script type="text/javascript">
    angular.module('app').value('apiVersion', "<%= apiVersion %>");
  </script>

  <script src="/static/routes.js" type="text/javascript"></script>
  <% for (var i in js) { %>
  <script src="/static<%= js[i] %>" type="text/javascript"></script>
  <% } %>

</body>
</html>
